  #分隔,下面是地区分组
  - name: 香港
    type: fallback
    include-all: true
    exclude-type: direct
    filter: "(?i)港|hk|hongkong|hong kong"
    url: "http://www.gstatic.com/generate_204"
    tolerance: 20
    health-check: 
      enable: true
      lazy: true  # 仅在节点超时或被使用时才测试

